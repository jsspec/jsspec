{
  "name": "@jsspec/jsspec",
  "version": "0.3.0",
  "description": "JSSpec - contextualised test runner for javascript",
  "main": "index.js",
  "scripts": {
    "test": "node bin/jsspec -r chai/register-expect",
    "coverage": "c8 --all=true --clean=true -r html -r text node bin/jsspec -r chai/register-expect -fo -b"
  },
  "bin": "bin/jsspec",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JSSpec/jsspec.git"
  },
  "keywords": [
    "spec",
    "test",
    "jsspec"
  ],
  "dependencies": {
    "@jsspec/cli-options": "0.0.8",
    "@jsspec/format": "^0.1.6",
    "glob": "^7.1.6",
    "pirates": "^4.0.1"
  },
  "devDependencies": {
    "c8": "^7.3.5",
    "chai": "^4.2.0",
    "sinon": "^9.2.1",
    "sinon-chai": "^3.5.0",
    "prettier": "^2.2.0"
  },
  "author": "HookyQR",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/JSSpec/jsspec/issues"
  },
  "homepage": "https://jsspec.github.io/",
  "files": [
    "bin",
    "src"
  ]
}
